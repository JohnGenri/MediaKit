{
  "telegram": {
    "bot_token": "123456789:YOUR_TELEGRAM_BOT_TOKEN",
    "admin_id": 123456789,
    "api_base_url": "https://tg.example.com",
    "timeouts": {
      "connect_sec": 30,
      "read_sec": 600,
      "write_sec": 600,
      "pool_sec": 30
    }
  },
  "database": {
    "USER": "db_user",
    "PASSWORD": "db_password",
    "HOST": "db-host.example.com",
    "PORT": "6432",
    "DB_NAME": "MediaKit"
  },
  "network": {
    "cookies": {
      "youtube": "important/www.youtube.com_cookies.txt",
      "reddit": "important/www.reddit.com_cookies.txt",
      "tiktok": "important/www.tiktok.com_cookies.txt"
    },
    "proxies": {
      "yandex": "http://user:pass@ip:port",
      "reddit": "socks5://user:pass@ip:port",
      "youtube": null,
      "tiktok": null,
      "pornhub": null
    },
    "headers": {
      "yandex_auth": "Bearer YOUR_YANDEX_MUSIC_TOKEN"
    }
  },
  "integrations": {
    "rapid_api": {
      "key": "YOUR_RAPID_API_KEY"
    },
    "reddit": {
      "client_id": "YOUR_REDDIT_CLIENT_ID",
      "client_secret": "YOUR_REDDIT_CLIENT_SECRET",
      "user_agent": "MediaKit/1.0",
      "proxy": "socks5://user:pass@ip:port"
    },
    "yandex": {
      "gpt": {
        "API_KEY": "YOUR_YANDEX_GPT_API_KEY",
        "FOLDER_ID": "YOUR_FOLDER_ID",
        "MODEL_URI": "gpt://YOUR_FOLDER_ID/yandexgpt/rc",
        "SYSTEM_PROMPT": "You are a summarization service."
      },
      "speechkit": {
        "API_KEY": "YOUR_YANDEX_SPEECHKIT_API_KEY",
        "FOLDER_ID": "YOUR_FOLDER_ID",
        "S3_BUCKET_NAME": "your-bucket-name",
        "S3_ACCESS_KEY_ID": "YOUR_S3_ACCESS_KEY",
        "S3_SECRET_ACCESS_KEY": "YOUR_S3_SECRET_KEY"
      }
    }
  },
  "limits": {
    "max_file_size_mb": 200,
    "download_concurrency": 4,
    "admin_button_chunk_size": 50,
    "cleanup_interval_sec": 3600,
    "cleanup_ttl_sec": 3600
  },
  "downloads": {
    "ytdlp": {
      "default_format": "bestvideo[ext=mp4][height<=720]+bestaudio[ext=m4a]/best[ext=mp4][height<=720]/best[height<=720]",
      "socket_timeout_sec": 30
    }
  },
  "messages": {
    "error_user": "Error. Try again later or check the link",
    "too_large": "File is too large (>200MB).",
    "start": "MediaBot Ready (DB Caching).",
    "status": {
      "analyzing": "Analyzing...",
      "sending": "Sending...",
      "listening": "Listening...",
      "album": "Album: {count} tracks..."
    }
  },
  "features": {
    "excluded_chats": [],
    "skip_conversion_services": [
      "YouTube",
      "TikTok",
      "PornHub",
      "Pinterest"
    ],
    "exact_matches": {
      "hello": "Hi there"
    }
  }
}
